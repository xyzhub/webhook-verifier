{
  "name": "@xyz/webhook-verifier",
  "version": "1.2.0",
  "description": "A lightweight library for verifying webhook signatures",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./configs": {
      "import": "./dist/configs/index.js",
      "require": "./dist/configs/index.js",
      "types": "./dist/configs/index.d.ts"
    },
    "./formatters": {
      "import": "./dist/formatters/index.js",
      "require": "./dist/formatters/index.js",
      "types": "./dist/formatters/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "webhook",
    "signature",
    "verification",
    "security"
  ],
  "author": "Baker @ XYZ",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xyzhub/webhook-verifier.git"
  },
  "bugs": {
    "url": "https://github.com/xyzhub/webhook-verifier/issues"
  },
  "homepage": "https://github.com/xyzhub/webhook-verifier#readme"
} 